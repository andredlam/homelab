modules:
    http_2xx:
        prober: http
        timeout: 5s
        http:
            fail_if_not_ssl: true
            ip_protocol_fallback: false
            method: GET
            no_follow_redirects: false
            preferred_ip_protocol: ip4
            valid_http_versions:
                - HTTP/1.1
                - HTTP/2.0
    http_post_2xx:
        prober: http
        timeout: 5s
        http:
            method: POST
            headers:
                Content-Type: application/json
                body: '{}'
    tcp_connect:
        prober: tcp
        timeout: 5s
    icmp:
        prober: icmp
        timeout: 5